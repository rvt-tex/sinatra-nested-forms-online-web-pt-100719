  <p>
    <label for="ship_name_2">Ship Name:</label>
    <input type="text" name="pirate[ships][][name]" id="ship_name_2" value="">
  </p>
  <p>
    <label for="ship_type_2">Ship Type:</label>
    <input type="text" name="pirate[ships][][type]" id="ship_type_2" value="">
  </p>
  <p>
    <label for="ship_booty_2">Ship Booty:</label>
